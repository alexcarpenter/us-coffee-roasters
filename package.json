{
  "name": "@alexcarpenter/us-coffee-roasters",
  "version": "1.0.0",
  "description": "A comprehensive list of US Coffee Roasters",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "data.json"
  ],
  "keywords": [
    "coffee",
    "roasters",
    "usa",
    "united-states",
    "data"
  ],
  "scripts": {
    "test": "node test/validate.js",
    "test:lint": "eslint .",
    "test:format": "prettier --check .",
    "format": "prettier --write .",
    "sort": "node scripts/sort.js",
    "test:all": "pnpm run test && pnpm run test:lint && pnpm run test:format"
  },
  "packageManager": "pnpm@9.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexcarpenter/us-coffee-roasters.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
